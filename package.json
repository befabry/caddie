{
  "name": "caddie",
  "version": "1.0.0",
  "description": "An app to help households with shopping",
  "main": "index.js",
  "scripts": {
    "format": "npx prettier --write \"{backend,client}/{src,test}/**/*.{ts,tsx}\"",
    "start": "npx concurrently \"npm:start:*\"",
