  "keywords": [
    "NestJs",
    "React"
  ],
  "author": "Benjamin Fabry",
  "contributors": [
    "Olivia Hick"
  ],
  "license": "ISC",
  "workspaces": [
    "client",
    "backend"
  ],
  "devDependencies": {
    "concurrently": "^6.2.1",
    "prettier": "^2.4.1"
  }
}
